MEMORY
{
	INES: start =     0, size =   $10, fill=yes, fillval=$00;
	PRG:  start = $8000, size = $8000, fill=no;
	CHR:  start =     0, size = $8000, fill=yes, fillval=$FF;
}

SEGMENTS
{
	INES:       load = INES,  type = ro;
	PRG:        load = PRG, type = ro;
	CHR:        load = CHR, type = ro;
}
